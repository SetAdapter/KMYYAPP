<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RateImageView">
        <attr name="rate_image" format="float" />
    </declare-styleable>

    <declare-styleable name="RateLayout">
        <attr name="rate_oritation" format="integer">
            <enum name="horization" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="rate_layout" format="float" />
    </declare-styleable>

    <declare-styleable name="GridLinearLayout">
        <attr name="column_count" format="integer" />
        <attr name="horizonal_space" format="dimension" />
        <attr name="vertical_space" format="dimension" />
        <attr name="horizonal_divider_mode" format="integer">
            <enum name="middle" value="1" />
            <enum name="normal" value="0" />
        </attr>
        <attr name="vertical_divider_mode" format="integer">
            <enum name="middle" value="1" />
            <enum name="normal" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PagerIndicator">
        <attr name="IndicatorNormal" format="color" />
        <attr name="IndicatorSelect" format="color" />
    </declare-styleable>
    <declare-styleable name="TagCloudLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="tagSpacing" format="dimension" />
        <!-- 是否是固定布局 -->
        <attr name="isFixed" format="boolean" />
        <attr name="columnSize" format="integer" />
    </declare-styleable>
</resources>